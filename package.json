{
  "name": "airseeker-v2",
  "version": "2.0.0",
  "keywords": [],
  "license": "MIT",
  "engines": {
    "node": "^18.14.0",
    "pnpm": "^8.14.0"
  },
  "files": [
    "dist",
    "*.js"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf coverage dist artifacts cache src/typechain-types",
    "contracts:compile:force": "hardhat compile --force",
    "contracts:compile": "hardhat compile",
    "dev:eth-node": "hardhat node",
    "dev": "nodemon --ext ts,js,json,env  --exec \"pnpm ts-node src/index.ts\"",
    "docker:build": "docker build --tag api3/airseeker:latest --file docker/Dockerfile .",
    "docker:build:amd64": "docker buildx build --platform linux/amd64 --tag api3/airseeker:latest --file docker/Dockerfile .",
    "docker:run": "docker run -it --init --volume $(pwd)/config:/app/config --network host --env-file .env --rm api3/airseeker:latest",
    "eslint:check": "eslint --report-unused-disable-directives --cache --ext js,ts . --max-warnings 0",
    "eslint:fix": "pnpm run eslint:check --fix",
    "prettier:check": "prettier --check \"./**/*.{js,ts,md,json}\"",
    "prettier:fix": "prettier --write \"./**/*.{js,ts,md,json}\"",
    "test:e2e": "jest --config=jest-e2e.config.js",
    "test": "jest --config=jest-unit.config.js",
    "tsc": "tsc --project ."
  },
  "devDependencies": {
    "@api3/airnode-abi": "^0.12.0",
    "@api3/ois": "^2.2.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts": "4.9.3",
    "@openzeppelin/merkle-tree": "^1.0.5",
    "@typechain/ethers-v5": "^11.1.2",
    "@typechain/hardhat": "^9.1.0",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "ethers": "^5.7.2",
    "hardhat": "^2.19.2",
    "hardhat-deploy": "^0.11.44",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@api3/airnode-protocol-v1": "^3.0.0",
    "@api3/commons": "^0.4.1",
    "@api3/dapi-management": "github:api3dao/dapi-management",
    "@api3/promise-utils": "^0.4.0",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "zod": "^3.22.3"
  }
}
